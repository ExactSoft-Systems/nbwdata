{% for student in row._entity.field_students %}&#10    <a class="use-ajax quick-view-link"&#10       data-student-id="{{ student.target_id }}"&#10       data-dialog-type="modal"&#10       data-dialog-options="{&quot;width&quot;: &quot;66%&quot;, &quot;height&quot;: &quot;66%&quot;}"&#10       href="/youth/quick_view/{{ student.target_id }}">{{ student.entity.label }}</a><br>&#10{% endfor %}&#10&#10<script>&#10  (function ($, Drupal) {&#10    Drupal.behaviors.quickViewModalCleanup = {&#10      attach: function (context, settings) {&#10        $('.quick-view-link', context).once('quickViewModalCleanup').on('click', function (e) {&#10          e.preventDefault();&#10          var link = $(this).attr('href');&#10          var studentId = $(this).data('student-id');&#10          var uniqueId = 'quick-view-modal-' + studentId + '-' + Date.now(); // Generate a unique ID using timestamp&#10&#10          // Create a new modal dialog with a unique ID for this instance&#10          $('<div id="' + uniqueId + '"></div>')&#10            .load(link + ' #content') // Load the Quick View mode content into the modal&#10            .dialog({&#10              title: 'Quick View - Student ' + studentId,&#10              width: '66%',&#10              height: '66%',&#10              modal: true,&#10              close: function () {&#10                // Properly destroy the dialog on close to ensure it can be reopened&#10                $(this).dialog('destroy').remove();&#10              }&#10            });&#10        });&#10      }&#10    };&#10  })(jQuery, Drupal);&#10</script>&#10